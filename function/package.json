{
  "name": "vodafone-backend",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "serverless package && npm run install-dependency-prod",
    "install-dependency-prod": "npm install --production",
    "sls:offline": "npm run sls:offline:dev",
    "sls:offline:dev": "SET NODE_ENV=development& serverless offline -s development",
    "sls:deploy:dev": "SET NODE_ENV=development& serverless deploy -s development",
    "swagger:generate": "ts-node scripts/runFullSwaggerPipeline.ts",
    "lint": "eslint --ext .ts --fix .",
    "prettier": "prettier --write --ignore-unknown .",
    "layer": "powershell -Command Remove-Item -Recurse -Force nodejs -ErrorAction SilentlyContinue; mkdir nodejs; if (-Not (Test-Path node_modules)) { npm install --production } ; Move-Item -Path node_modules -Destination nodejs/",
    "build-zip": "mkdir nodejs && cd nodejs && npm install --production && cd .. && powershell Compress-Archive -Path nodejs -DestinationPath nodejs.zip -Force"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@serverless/typescript": "^3.38.0",
    "@types/aws-lambda": "^8.10.134",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.17.50",
    "@types/passport": "^1.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^10.0.0",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.3",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.51.0",
    "serverless-offline": "^13.3.3",
    "serverless-offline-watcher": "^1.1.0",
    "serverless-plugin-diff": "^3.0.0",
    "serverless-plugin-typescript": "^2.1.5",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-connect": "^3.774.0",
    "@aws-sdk/client-dynamodb": "^3.624.0",
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/client-ses": "^3.758.0",
    "@aws-sdk/lib-dynamodb": "^3.624.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "http-status-codes": "^2.3.0",
    "libphonenumber-js": "^1.12.6",
    "log4js": "^6.9.1",
    "moment": "^2.30.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "serverless-http": "^3.2.0",
    "tsx": "^4.16.5",
    "uuid": "^10.0.0"
  }
}
